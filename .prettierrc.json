{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*/privacy-policy/page.tsx",
      "options": {
        "printWidth": 1000000
      }
    },
    {
      "files": "*/terms-and-conditions/page.tsx",
      "options": {
        "printWidth": 1000000
      }
    }
  ]
}
